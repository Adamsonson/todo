<p style="color: green"><%= notice %></p>

<h1>Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>State</th>
      <th colspan="2"></th>
    </tr>
  </thead>
  <tbody>
    <div id="tasks">
      <% @tasks.each do |task| %>
        <%= render task %>
      <% end %>
    </div>
  </tbody>
</table>

<br>

<%= link_to "New task", new_task_path %>
